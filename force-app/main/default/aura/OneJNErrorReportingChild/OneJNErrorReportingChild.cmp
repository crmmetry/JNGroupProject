<aura:component>
  <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
  <aura:attribute name="map" type="map" />
  <aura:attribute name="jsonMap" type="map" />
  <aura:attribute name="key" type="string" />
  <aura:attribute name="errorMessage" type="string" />
  <aura:attribute name="errorCode" type="string" />
  <aura:attribute name="eventName" type="string" />
  <aura:attribute name="time" type="string" />
  <aura:attribute name="date" type="string" />
  <aura:attribute name="userName" type="string" />

  <li>
    <div class="slds-timeline__item_expandable slds-timeline__item_task">
      <span class="slds-assistive-text">task</span>
      <div class="slds-media">
        <div class="slds-media__figure" style="margin-left: 5%;">
          <div
            class="slds-icon_container slds-icon-standard-task slds-timeline__icon"
            title="task"
          >
            <lightning:icon iconName="standard:task" size="medium" />
          </div>
        </div>
        <div class="slds-media__body">
          <div class="slds-grid slds-grid_align-spread slds-timeline__trigger">
            <div
              class="slds-grid slds-grid_vertical-align-center slds-truncate_container_75 slds-no-space"
            >
              <h3 class="slds-truncate" title="{!v.eventName}">
                <strong>{!v.eventName}</strong>
              </h3>
            </div>
            <div class="slds-timeline__actions slds-timeline__actions_inline">
              <p class="slds-timeline__date">{!v.time} | {!v.date}</p>
            </div>
          </div>
          <p class="slds-m-horizontal_xx-small">
            {!v.errorMessage}<br />
            {!v.errorCode} - {!v.userName}
          </p>
        </div>
      </div>
    </div>
  </li>
</aura:component>
